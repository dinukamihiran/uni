<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="initial-scale=1.0, user-scalable=no">
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">
  <title></title>
  
  
  
  <link rel="stylesheet" href="https://d10ajoocuyu32n.cloudfront.net/mobile/1.3.1/jquery.mobile-1.3.1.min.css">
  
  <!-- Extra Codiqa features -->
  <link rel="stylesheet" href="codiqa.ext.css">
  
  <!-- jQuery and jQuery Mobile -->
  <script src="https://d10ajoocuyu32n.cloudfront.net/jquery-1.9.1.min.js"></script>
  <script src="https://d10ajoocuyu32n.cloudfront.net/mobile/1.3.1/jquery.mobile-1.3.1.min.js"></script>

  <!-- Extra Codiqa features -->
  <script src="https://d10ajoocuyu32n.cloudfront.net/codiqa.ext.js"></script>
   
</head>
<body>
<!-- Home -->
<div data-role="page" id="page1">
    <div data-theme="c" data-role="header" data-position="fixed">
        <h3>
            Welcome to SiS
        </h3>
    </div>
    <div data-role="content">
        <div data-role="fieldcontain">
            <label for="searchinput3">
                Search
            </label>
            <input name="" id="searchinput3" placeholder="Index number or name" value=""
            type="search">
        </div>
        <div data-role="fieldcontain">
            <label for="selectmenu1">
                Choose:
            </label>
            <select id="selectmenu1" name="">
                <option value="option1">
                    Option 1
                </option>
            </select>
        </div>
        <div data-role="fieldcontain">
            <select name="toggleswitch1" id="toggleswitch1" data-theme="" data-role="slider">
                <option value="off">
                    Off
                </option>
                <option value="on">
                    On
                </option>
            </select>
        </div>
        <div data-role="fieldcontain">
            <label for="slider1">
                Value
            </label>
            <input id="slider1" name="slider" value="50" min="0" max="100" data-highlight="false"
            type="range">
        </div>
        <div data-role="fieldcontain">
            <fieldset data-role="controlgroup" data-type="vertical">
                <legend>
                    Choose:
                </legend>
                <input id="radio1" name="" value="" data-theme="c" type="radio">
                <label for="radio1">
                    Option
                </label>
            </fieldset>
        </div>
    </div>
    <div data-theme="b" data-role="footer" data-position="fixed">
        <h3>
            About
        </h3>
    </div>
</div>
</body>
</html>